{
  "name": "full-stack-fusion",
  "version": "0.0.1",
  "scripts": {
    "start": "node back-end --ignore front-end",
    "develop": "concurrently \"cd back-end && npm run start\" \"cd front-end && npm run dev\"",
    "install": "cd back-end && npm i && cd ../front-end && npm i",
    "seed": "cd back-end && npm run seed",
    "build": "cd front-end && npm run build",
    "render-build":"npm install && npm run build"
  },
  "keywords": [],
